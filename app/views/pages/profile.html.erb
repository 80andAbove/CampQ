 <div class="container py-3">
    <ul class="nav tabs-underlined" id="myTab" role="tablist">
      <li class="nav-item" role="presentation">
        <a class="active tab-underlined" id="home-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="true">My profile</a>
      </li>
      <li class="nav-item" role="presentation">
        <a class="tab-underlined" id="home-tab" data-toggle="tab" href="#bookings" role="tab" aria-controls="home" aria-selected="true">Requests</a>
      </li>
      <li class="nav-item" role="presentation">
        <a class="tab-underlined" id="profile-tab" data-toggle="tab" href="#requests" role="tab" aria-controls="profile" aria-selected="false">My rentals</a>
      </li>
       <li class="nav-item" role="presentation">
        <a class="tab-underlined" id="list-tab" data-toggle="tab" href="#lists" role="tab" aria-controls="profile" aria-selected="false">My posts</a>
      </li>
    </ul>
    <div class="tab-content" id="myTabContent">
      <div class="tab-pane fade py-3" id="requests" role="tabpanel" aria-labelledby="profile-tab">
        <h1 class="m-3">My profile</h1>

      </div>
      <div class="tab-pane fade py-3 show active" id="bookings" role="tabpanel" aria-labelledby="home-tab">
        <h1 class="m-3">Requests</h1>

        <%= render 'gear_card', bookings: @bookings %>

      </div>
      <div class="tab-pane fade py-3" id="requests" role="tabpanel" aria-labelledby="profile-tab">
        <h1 class="m-3">My rentals</h1>

        <%= render 'gear_card', bookings: @rental_bookings %>
      </div>
      <div class="tab-pane fade py-3" id="lists" role="tabpanel" aria-labelledby="list-tab">
        <h1 class="m-3">My posts</h1>

        <h2 class="m-3">No products listed. Create your first post by clicking on the button in the top right corner.</h2>

      </div>
    </div>

 <h1>Bookings calendar</h1>

    <div>
     <%= month_calendar(events: @bookings+@rental_bookings, attribute: :start_time, end_attribute: :end_time) do |date, bookings| %>
                <%= date %>
                <% bookings.each do |booking| %>
                  <div class="booking-profile-calendar">
                    <p><%= booking.product.name %></p>
                  </div>
                <% end %>
              <% end %>
    </div>
  </div>





 </div>



